.heading
  %h1 Create New Tour

- form_for @changeset, current_user_tour_path(@conn, :create), fn f ->
  = render "_form.html", f: f

  - cancel = link "Cancel", to: current_user_tour_path(@conn, :index), class: "btn btn-link"
  = PBF.submit f, "Create Tour", class: "btn-primary", alternative: cancel
